ings from \"./componets/Setings/Setings\";\nimport {BrowserRouter, Route} from \"react-router-dom\";\nimport DialogsContainer from \"./componets/Dialogs/DialogsContainer\";\nimport UsersContainer from \"./componets/Users/UsersContainer\";\nimport ProfileContainer from \"./componets/Profile/ProfileContainer\";\nimport LoginPage from \"./Login/Login\";\n\n\n\n\nconst App = () => {\n    return (\n\n            <div className='app-wrapper'>\n                <HeaderContainer />\n                <Navbar />\n\n                <div className='app-wrapper-content'>\n\n                    <Route path='/dialogs'\n                           render ={ () => <DialogsContainer /> }/>\n\n                    <Route path='/profile/:userId?'\n                                render ={ () => <ProfileContainer /> } />\n\n                    <Route path='/users'\n                           render ={ () => <UsersContainer /> } />\n\n                     <Route path='/login'\n                           render ={ () => <LoginPage /> } />\n\n                    <Route path='/news' render ={() => <News />} />\n                    <Route path='/music' render ={ () => <Music /> } />\n                    <Route path='/setings' render ={ () => <Setings />} />\n\n                </div>\n\n            </div>\n    );\n}\n\nexport default App;\n","D:\\react-kabzda\\react-social-networks\\src\\componets\\Header\\Header.jsx",["149"],"import React from 'react';\nimport s from './Header.module.css';\nimport {NavLink} from \"react-router-dom\";\nconst Header = (props) => {\n    return (\n            <header className={s.header}>\n                <img\n                    src='http://3.bp.blogspot.com/-iKHwpJF-EHo/UE17RbszugI/AAAAAAAAAN4/THNcHg8V2Zc/s1600/twitter-bird-light-bgs.png'/>\n\n                    <div className={s.loginBlock}>\n                        { props.isAuth ?  props.login\n                        : <NavLink to={'/login'}>Login</NavLink> }\n                    </div>\n            </header