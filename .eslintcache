us));\r\n        }\r\n    });\r\n}\r\n\r\nexport default profileReducer;","D:\\react-kabzda\\react-social-networks\\src\\redax\\sidebar_reducer.js",[],"D:\\react-kabzda\\react-social-networks\\src\\redax\\dialogs_reducer.js",[],"D:\\react-kabzda\\react-social-networks\\src\\redax\\auth_reducer.js",[],"D:\\react-kabzda\\react-social-networks\\src\\redax\\users_reducer.js",[],"D:\\react-kabzda\\react-social-networks\\src\\Login\\Login.jsx",[],"D:\\react-kabzda\\react-social-networks\\src\\componets\\Header\\HeaderContainer.jsx",[],"D:\\react-kabzda\\react-social-networks\\src\\componets\\News\\News.jsx",[],"D:\\react-kabzda\\react-social-networks\\src\\componets\\Music\\Music.jsx",[],"D:\\react-kabzda\\react-social-networks\\src\\componets\\Setings\\Setings.jsx",[],"D:\\react-kabzda\\react-social-networks\\src\\componets\\Users\\UsersContainer.jsx",[],"D:\\react-kabzda\\react-social-networks\\src\\componets\\Navbar\\Navbar.jsx",[],"D:\\react-kabzda\\react-social-networks\\src\\componets\\Profile\\ProfileContainer.jsx",[],"D:\\react-kabzda\\react-social-networks\\src\\componets\\Dialogs\\DialogsContainer.jsx",["169","170"],"import React from 'react';\nimport {sendMessageCreator} from \"../../redax/dialogs_reducer\";\nimport Dialogs from \"./Dialogs\";\nimport {connect} from \"react-redux\";\nimport {Redirect} from \"react-router-dom\";\nimport {withAuthRedirect} from \"../../hoc/withAuthRedirect\";\nimport {compose} from \"redux\";\n\n\nlet mapStateToProps = (state) =>{\n    return{\n        dialogsPage: state.dialogsPage\n    }\n}\nlet mapDispatchToProps = (dispatch) =>{\n    return{\n        sendMessage: (newMessageBody) =>{\n            dispatch(sendMessageCreator(newMessageBody));\n        }\n    }\n}\n\n\n\nexport default compose(\n    connect(mapStateToProps, mapDispatchToProps),\n    withAuthRedirect\n)(Dialogs);","D:\\react-kabzda\\react-social-networks\\src\\api\\api.js",[],"D:\\react-kabzda\\react-social-networks\\src\\componets\\common\\Preloader.js",["171"]